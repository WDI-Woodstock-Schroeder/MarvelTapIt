<body id="welcomeBody">
  <nav class='login'>
    <h1>Marvel Tap-It</h1>
  </nav>

  <div class="container landing">
    <div class="row">
      <div class="twelve columns">

        <%= form_tag('/log_in', method: :post) do %>
        <%= label_tag :username %>
        <%= text_field_tag :username, 'guest' %>
        <%= label_tag :password %>
        <%= password_field_tag :password, 'password' %>
        <br>
        <%= submit_tag "Sign-In" %>

        <% end %>
      </div>
    </div>
  </div>
</body>
